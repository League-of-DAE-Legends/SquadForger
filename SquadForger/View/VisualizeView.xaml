      xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
      xmlns:local="clr-namespace:SquadForger.View"
      xmlns:vm="clr-namespace:SquadForger.ViewModel"
      mc:Ignorable="d"
      d:DesignWidth="800" d:DesignHeight="450"
      Title="Visualize View">
